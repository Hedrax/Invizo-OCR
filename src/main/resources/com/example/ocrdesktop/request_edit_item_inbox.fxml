<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.text.*?>

<HBox alignment="CENTER" spacing="10.0" xmlns="http://javafx.com/javafx/17.0.2-ea" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.example.ocrdesktop.ui.subelements.RequestEditItemInbox">
   <padding>
      <Insets left="12.0" />
   </padding>
   <Label fx:id="columnName" text="Column name">
      <font>
         <Font name="Arial" size="14.0" />
      </font>
   </Label>
   <Label text=":">
      <font>
         <Font name="System Bold" size="14.0" />
      </font>
   </Label>
   <TextField fx:id="value" HBox.hgrow="ALWAYS">
      <font>
         <Font size="14.0" />
      </font></TextField>
</HBox>
